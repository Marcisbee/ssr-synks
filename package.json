{
  "name": "ssr-synks",
  "version": "0.1.0",
  "description": "",
  "main": "src/core/index.js",
  "module": "src/core/index.js",
  "engines": {
    "node": ">=12.x"
  },
  "bin": {
    "ssr": "bin/ssr.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "npm start --prefix ./app",
    "test": "jest",
    "lint": "eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Marcisbee/ssr-synks.git"
  },
  "author": "Marcis <marcisbergmanis@gmail.com> Bergmanis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Marcisbee/ssr-synks/issues"
  },
  "homepage": "https://github.com/Marcisbee/ssr-synks#readme",
  "dependencies": {
    "esbuild": "^0.4.2",
    "node-cookie": "^2.1.2",
    "ws": "^7.3.0"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^26.0.1"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true
    },
    "extends": [
      "airbnb-base"
    ],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 11
    },
    "overrides": [
      {
        "files": [
          "**/*.test.js"
        ],
        "env": {
          "jest": true
        }
      }
    ],
    "rules": {
      "import/prefer-default-export": "off",
      "no-underscore-dangle": "off",
      "no-restricted-syntax": "off",
      "no-param-reassign": "off",
      "no-return-assign": "off",
      "no-await-in-loop": "off",
      "guard-for-in": "off"
    }
  }
}
